*{margin:0; padding:0;}
body{margin:0 auto; background:#fff url(../images/bg.jpg) repeat; font-family:宋体;}
a{text-decoration:none;}
ul{list-style:none;}
h1,h2{font-weight:normal;}
#top{margin:0 auto; width:988px; height:86px; background:url(../images/topbg2.png) no-repeat top center;}
.topUl{width:208px; height:12px; padding-top:2px; float:right; padding-right:14px;}
.topUl li{width:52px; height:12px; font-size:12px;  float:left;}
.topli_1{padding-right:14px; background:url(../images/liline.gif) no-repeat center right;}
.topli_2{padding-left:12px; padding-right:14px; background:url(../images/liline.gif) no-repeat center right;}
.topli_3{padding-left:12px;}
.topUl li a{font-size:12px; width:52px; line-height:12px; float:left;  color:#204890;}
.topsign{background:url(../images/topsign.png); height:64px; width:61px; margin-top:11px;float:left; margin-left:14px;}
.shyzsign{background:url(../images/shyzsign.png); height:70px; width:153px;float:left ;margin-top:3px; margin-left:12px;}
.topsearch{width:350px; height:31px; background:#d9f0ff; float:left; margin-top:30px; margin-left:64px; padding-left:3px;}
#topsort{width:88px;  height:25px; float:left; border:1px solid #97b4e0; background-color:#fff;  color:#204890;margin-top:2px; }
.sort{width:71px;  height:25px; float:left; text-align:center; line-height:25px; font-size:14px;}
.sortclick{width:15px; height:25px; border-left:1px solid #97b4e0; background:url(../images/sortclick.gif) no-repeat; float:right; cursor:pointer;}
.sortul{width:86px;background:#fff;border:1px solid #97b4e0; float:left; position:relative;  z-index:1; text-align:center; display:none;}
.sortul li{cursor:pointer;  height:25px; font-size:14px; line-height:25px;}
.topinput{width:251px; height:27px; float:left; margin-left:6px;background-color:#fff; margin-top:2px; display:inline;}
.inputtext{width:197px; height:25px; line-height:25px; font-size:12px; border:1px solid #97b4e0; border-right:1px none; float:left;}
.inputbutton{width:52px; height:27px; float:left; border:1px  solid #97b4e0; background:url(../images/inputbutton.gif); cursor:pointer}
.topUl1{width:223px; height:22px; line-height:22px; margin-top:50px;  padding-left:14px; float:right; background:url(../images/topUl1bg.png) no-repeat;}
.topUl1 li{  float:left; padding-right:19px;}
#ul1LiLast{padding-right:0px;}
.topUl1 li a{color:#ffffff;  font-size:12px; height:22px; line-height:22px;}

#banner{width:100%; height:45px; background:url(../images/bannerbg.png) repeat-x center;}
.bannerUl{width:987px; height:45px; margin:0 auto;}
.bannerUl li{width:141px; height:45px; float:left; text-align:center; }
.bannerUl li a{line-height:45px; height:45px;font-family:微软雅黑; font-size:18px; color:#ffffff; }

#adBanner{width:960px; height:41px; margin:0 auto; border:1px solid #add5ef; border-top:0px none; background:#f8fdff url(../images/adBannerbg.png) repeat-x;}
.adBannerCenter{width:958px; height:31px; margin:0 auto;position:relative; overflow:hidden; margin-top:8px; border:1px solid #f6fafd; background:url(../images/adsign.gif) no-repeat 8px center; float:left;}
.adBannerCenter ul{width:660px; height:31px; position:absolute; top:0px; left:40px;}
.adBannerCenter ul li{height:31px; line-height:31px; font-size:12px;  }
.adBannerCenter ul li a{ color:#204890;margin-right:40px;}
.adButton{width:101px; height:25px; float:right; margin-right:20px; margin-top:3px; border:0px none; font-size:14px; color:#ffffff; background:url(../images/adbuttonbg.png); cursor:pointer;}

#footer{width:100%; clear:both; height:198px; background:#f2f2f2; font-family:宋体; border-top:1px solid #dbdbdb;}
.footerul{width:588px; height:32px; line-height:32px; margin:0 auto; clear:both;}
.footerul li{  width:98px; text-align:center; float:left;}
.footerul li a{color:#453d3d; text-align:center;  font-size:13px;}
.footerp{width:960px; margin:0 auto; clear:both; height:32px; line-height:32px; color:#453d3d; font-size:13px; text-align:center;}
.touchspan{padding-left:36px;}
.spanfrist{padding-left:0px;}
/* 部分网页右边*/
#right_content{height:100%; width:299px; float:right;}
.hengda{margin-top:32px;}
 .line{margin-top:16px;}
 .content_right_h1{font-size:14px; padding-left:15px; margin-top:12px;}
 .information{height:94px; width:263px; margin-top:20px; padding-left:15px;}
 .information p{font-size:14px; line-height:25px; width:237px;}
 .p_color1 a{color:#005e96;}
 .p_color2{color:#626262;}
 .p_color2 span{color:#e40001;}
.select{width:214px;height:31px; background:url(../images/lianj1.png) no-repeat left top; line-height:31px; color:#b1b1b1; float:left; margin-left:8px; overflow:hidden}
.select a{color:#b2b2b2;}
.select1{background:url(../images/lianj2.png) no-repeat left top;}
.select2{width:117px; background:url(../images/lianj3.png) no-repeat left top; float:left;}
.select3{ margin-left:15px;}
.select p{text-indent:30px; width:214px;height:25px;}
.select  ul{height:185px; width:210px; position:absolute;}
.select2 p{width:117px;}
.select2 ul{width:117px;}
.select2 li{width:117px;}
.sspan{padding-left:21px;}
.emailspan{padding-left:28px;}